//java
class Solution {
    public int hammingWeight(int n) {
        int ans=0;
        while(n!=0)
        {
            ans++;
            n&=n-1;
            System.out.println(n);
        }
        return ans+1;
    }
}
//C++
class Solution {
public:
    int hammingWeight(uint32_t n) {
        int ans=0;
        while(n)
        {
            ans++;
            n&=n-1;
        }
        return ans;
    }
};
